<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.*?>
<?import javafx.scene.control.*?>

<GridPane xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.Controller">
	<Label text = "Welcome" GridPane.columnIndex = "0" GridPane.rowIndex = "0"></Label>
	
	<Label text = "E-mail: " GridPane.columnIndex = "0" GridPane.rowIndex = "1"></Label>
	<TextField GridPane.columnIndex = "0" GridPane.rowIndex = "2" fx:id="email"></TextField>
	<Label text = "Password: " GridPane.columnIndex = "0" GridPane.rowIndex = "3"></Label>
	<PasswordField GridPane.columnIndex = "0" GridPane.rowIndex = "4" fx:id="password"></PasswordField>
	
	<Button text = "Enter" GridPane.columnIndex = "0" GridPane.rowIndex = "5" onAction="#enter">
	
	</Button>
</GridPane>

